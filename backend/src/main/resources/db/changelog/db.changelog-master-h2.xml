<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
          http://www.liquibase.org/xml/ns/dbchangelog
          http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd">

    <!--
      Siin ei viitsi me käsitsi kõiki faile loetleda.
      includeAll võtab KÕIK failid kaustast 'changes' (selle masteri suhtes).
      Failid jooksevad järjestuse järgi (001..., 002..., 2025-12-01-... jne),
      seega praegune numeratsioon töötab ideaalselt.
    -->
    <includeAll path="changes" relativeToChangelogFile="true"/>
</databaseChangeLog>
